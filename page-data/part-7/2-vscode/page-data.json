perties":{"className":["language-text"]},"children":[{"type":"text","value":"git config --global user.email \"your-email@example.com\""}]}]},{"type":"element","tagName":"td","properties":{},"children":[{"type":"text","value":"Set your Git email (must match GitHub)"}]}]},{"type":"element","tagName":"tr","properties":{},"children":[{"type":"element","tagName":"td","properties":{},"children":[{"type":"element","tagName":"code","properties":{"className":["language-text"]},"children":[{"type":"text","value":"git config --list"}]}]},{"type":"element","tagName":"td","properties":{},"children":[{"type":"text","value":"View your Git configuration"}]}]},{"type":"element","tagName":"tr","properties":{},"children":[{"type":"element","tagName":"td","properties":{},"children":[{"type":"element","tagName":"code","properties":{"className":["language-text"]},"children":[{"type":"text","value":"git init"}]}]},{"type":"element","tagName":"td","properties":{},"children":[{"type":"text","value":"Initialize a new Git repository in a folder"}]}]},{"type":"element","tagName":"tr","properties":{},"children":[{"type":"element","tagName":"td","properties":{},"children":[{"type":"element","tagName":"code","properties":{"className":["language-text"]},"children":[{"type":"text","value":"git clone <repo-url>"}]}]},{"type":"element","tagName":"td","properties":{},"children":[{"type":"text","value":"Clone a repository from GitHub"}]}]},{"type":"element","tagName":"tr","properties":{},"children":[{"type":"element","tagName":"td","properties":{},"children":[{"type":"element","tagName":"code","properties":{"className":["language-text"]},"children":[{"type":"text","value":"git status"}]}]},{"type":"element","tagName":"td","properties":{},"children":[{"type":"text","value":"Check the status of your working directory"}]}]},{"type":"element","tagName":"tr","properties":{},"children":[{"type":"element","tagName":"td","properties":{},"children":[{"type":"element","tagName":"code","properties":{"className":["language-text"]},"children":[{"type":"text","value":"git add <file>"}]}]},{"type":"element","tagName":"td","properties":{},"children":[{"type":"text","value":"Stage a file for commit"}]}]},{"type":"element","tagName":"tr","properties":{},"children":[{"type":"element","tagName":"td","properties":{},"children":[{"type":"element","tagName":"code","properties":{"className":["language-text"]},"children":[{"type":"text","value":"git add ."}]}]},{"type":"element","tagName":"td","properties":{},"children":[{"type":"text","value":"Stage all modified files for commit"}]}]},{"type":"element","tagName":"tr","properties":{},"children":[{"type":"element","tagName":"td","properties":{},"children":[{"type":"element","tagName":"code","properties":{"className":["language-text"]},"children":[{"type":"text","value":"git commit -m \"Your commit message\""}]}]},{"type":"element","tagName":"td","properties":{},"children":[{"type":"text","value":"Commit changes with a message"}]}]},{"type":"element","tagName":"tr","properties":{},"children":[{"type":"element","tagName":"td","properties":{},"children":[{"type":"element","tagName":"code","properties":{"className":["language-text"]},"children":[{"type":"text","value":"git log --oneline --graph --all"}]}]},{"type":"element","tagName":"td","properties":{},"children":[{"type":"text","value":"View commit history in a readable format"}]}]},{"type":"element","tagName":"tr","properties":{},"children":[{"type":"element","tagName":"td","properties":{},"children":[{"type":"element","tagName":"code","properties":{"className":["language-text"]},"children":[{"type":"text","value":"git branch"}]}]},{"type":"element","tagName":"td","properties":{},"children":[{"type":"text","value":"List all branches in the repository"}]}]},{"type":"element","tagName":"tr","properties":{},"children":[{"type":"element","tagName":"td","properties":{},"children":[{"type":"element","tagName":"code","properties":{"className":["language-text"]},"children":[{"type":"text","value":"git checkout -b <branch-name>"}]}]},{"type":"element","tagName":"td","properties":{},"children":[{"type":"text","value":"Create and switch to a new branch"}]}]},{"type":"element","tagName":"tr","properties":{},"children":[{"type":"element","tagName":"td","properties":{},"children":[{"type":"element","tagName":"code","properties":{"className":["language-text"]},"children":[{"type":"text","value":"git merge <branch-name>"}]}]},{"type":"element","tagName":"td","properties":{},"children":[{"type":"text","value":"Merge a branch into the current branch"}]}]},{"type":"element","tagName":"tr","properties":{},"children":[{"type":"element","tagName":"td","properties":{},"children":[{"type":"element","tagName":"code","properties":{"className":["language-text"]},"children":[{"type":"text","value":"git push origin <branch-name>"}]}]},{"type":"element","tagName":"td","properties":{},"children":[{"type":"text","value":"Push changes to GitHub"}]}]},{"type":"element","tagName":"tr","properties":{},"children":[{"type":"element","tagName":"td","properties":{},"children":[{"type":"element","tagName":"code","properties":{"className":["language-text"]},"children":[{"type":"text","value":"git pull origin <branch-name>"}]}]},{"type":"element","tagName":"td","properties":{},"children":[{"type":"text","value":"Pull the latest changes from GitHub"}]}]},{"type":"element","tagName":"tr","properties":{},"children":[{"type":"element","tagName":"td","properties":{},"children":[{"type":"element","tagName":"code","properties":{"className":["language-text"]},"children":[{"type":"text","value":"git remote -v"}]}]},{"type":"element","tagName":"td","properties":{},"children":[{"type":"text","value":"Show linked remote repositories"}]}]},{"type":"element","tagName":"tr","properties":{},"children":[{"type":"element","tagName":"td","properties":{},"children":[{"type":"element","tagName":"code","properties":{"className":["language-text"]},"children":[{"type":"text","value":"git reset --hard <commit-id>"}]}]},{"type":"element","tagName":"td","properties":{},"children":[{"type":"text","value":"Reset to a specific commit (dangerous, use with caution)"}]}]}]}]},{"type":"element","tagName":"p","properties":{},"children":[{"type":"element","tagName":"strong","properties":{},"children":[{"type":"text","value":"For a complete list of Git commands, check the official Git documentation:"}]},{"type":"text","value":"\n"},{"type":"element","tagName":"a","properties":{"href":"https://git-scm.com/docs","target":"_blank","rel":["noopener","noreferrer"]},"children":[{"type":"text","value":"Git Command Reference"}]}]}]},"html":"<div><text-box variant='learningObjectives' name=\"Learning objectives\"><p>After this section</p><ul>\n<li>You will know how to <strong>install and configure Git</strong> on your local machine.</li>\n<li>You will understand the role of <strong>Git in version control</strong> and how it integrates with <strong>GitHub</strong>.</li>\n<li>You will be able to <strong>set up and authenticate GitHub in VS Code</strong> for seamless collaboration.</li>\n<li>You will know how to <strong>clone a GitHub repository</strong>, make changes, and commit them back.</li>\n<li>You will be able to use <strong>GitHub Codespaces</strong> as an alternative cloud-based development environment.</li>\n<li>You will understand the differences between <strong>local Git workflows</strong> and <strong>cloud-based workflows (Codespaces)</strong>.</li>\n</ul></text-box><p>To work more effectively with GitHub, it’s essential to integrate it with an IDE like <strong>VS Code</strong>. There are two ways to do this:</p><hr><h2 id=\"method-1--local-setup\" style=\"position:relative;\">Method 1:  Local Setup<a href=\"#method-1--local-setup\" aria-label=\"method 1  local setup permalink\" class=\"anchor after\"><svg aria-hidden=\"true\" focusable=\"false\" height=\"16\" version=\"1.1\" viewBox=\"0 0 16 16\" width=\"16\"><path fill-rule=\"evenodd\" d=\"M4 9h1v1H4c-1.5 0-3-1.69-3-3.5S2.55 3 4 3h4c1.45 0 3 1.69 3 3.5 0 1.41-.91 2.72-2 3.25V8.59c.58-.45 1-1.27 1-2.09C10 5.22 8.98 4 8 4H4c-.98 0-2 1.22-2 2.5S3 9 4 9zm9-3h-1v1h1c1 0 2 1.22 2 2.5S13.98 12 13 12H9c-.98 0-2-1.22-2-2.5 0-.83.42-1.64 1-2.09V6.25c-1.09.53-2 1.84-2 3.25C6 11.31 7.55 13 9 13h4c1.45 0 3-1.69 3-3.5S14.5 6 13 6z\"></path></svg></a></h2><p>This method involves installing Git on your local machine and using the <strong>GitHub Pull Request and Issues extension</strong> in VS Code.</p><h3 id=\"steps-to-set-up-git-with-vs-code\" style=\"position:relative;\"><strong>Steps to Set Up Git with VS Code:</strong><a href=\"#steps-to-set-up-git-with-vs-code\" aria-label=\"steps to set up git with vs code permalink\" class=\"anchor after\"><svg aria-hidden=\"true\" focusable=\"false\" height=\"16\" version=\"1.1\" viewBox=\"0 0 16 16\" width=\"16\"><path fill-rule=\"evenodd\" d=\"M4 9h1v1H4c-1.5 0-3-1.69-3-3.5S2.55 3 4 3h4c1.45 0 3 1.69 3 3.5 0 1.41-.91 2.72-2 3.25V8.59c.58-.45 1-1.27 1-2.09C10 5.22 8.98 4 8 4H4c-.98 0-2 1.22-2 2.5S3 9 4 9zm9-3h-1v1h1c1 0 2 1.22 2 2.5S13.98 12 13 12H9c-.98 0-2-1.22-2-2.5 0-.83.42-1.64 1-2.09V6.25c-1.09.53-2 1.84-2 3.25C6 11.31 7.55 13 9 13h4c1.45 0 3-1.69 3-3.5S14.5 6 13 6z\"></path></svg></a></h3><ol>\n<li>\n<p><strong>Install Git</strong> (Version Control System)  (if not installed)</p>\n<ul>\n<li>Download and install Git from <a href=\"https://git-scm.com/downloads\" target=\"_blank\" rel=\"noopener noreferrer\">Git Downloads</a>.</li>\n<li>After installation, verify the installation by running:\n<div class=\"gatsby-highlight\" data-language=\"sh\"><pre class=\"language-sh\"><code class=\"language-sh\"><span class=\"token function\">git</span> <span class=\"token parameter variable\">--version</span></code></pre></div>\n</li>\n</ul>\n</li>\n<li>\n<p><strong>Install VS Code</strong> (if not installed)</p>\n<ul>\n<li>Download it from <a href=\"https://code.visualstudio.com/\" target=\"_blank\" rel=\"noopener noreferrer\">Visual Studio Code</a>.</li>\n</ul>\n</li>\n<li>\n<p><strong>Install GitHub Pull Request and Issues Extension</strong></p>\n<ul>\n<li>Open VS Code and go to the <strong>Extensions Marketplace</strong> (<code class=\"language-text\">Ctrl + Shift + X</code> / <code class=\"language-text\">Cmd + Shift + X</code> on macOS).</li>\n<li>Search for <strong>GitHub Pull Requests and Issues</strong> and install it.</li>\n<li>Alternatively, install it directly from <a href=\"https://marketplace.visualstudio.com/items?itemName=GitHub.vscode-pull-request-github\" target=\"_blank\" rel=\"noopener noreferrer\">VS Code Extensions</a>.</li>\n</ul>\n</li>\n<li>\n<p><strong>Authenticate GitHub in VS Code</strong></p>\n<ul>\n<li>Open VS Code and navigate to:\n<div class=\"gatsby-highlight\" data-language=\"text\"><pre class=\"language-text\"><code class=\"language-text\">View > Command Palette (Ctrl + Shift + P / Cmd + Shift + P on macOS)</code></pre></div>\n</li>\n<li>Search for <strong>\"GitHub: Sign in to GitHub\"</strong> and follow the prompts.</li>\n</ul>\n</li>\n<li>\n<p><strong>Clone a GitHub Repository into VS Code</strong></p>\n<ul>\n<li>Open VS Code and open the <strong>Command Palette</strong>.</li>\n<li>Select <strong>\"Git: Clone\"</strong> and enter the repository URL from GitHub.  (Example Repo Link: <a href=\"https://github.com/Laurea-amk-Leppavaara/github-starter-space\" target=\"_blank\" rel=\"noopener noreferrer\">https://github.com/Laurea-amk-Leppavaara/github-starter-space</a>)</li>\n<li>Choose a local folder to save the repository and start coding.</li>\n</ul>\n</li>\n</ol><p><strong>Read More: For full instructions, refer to the official VS Code guide:</strong>\n<a href=\"https://code.visualstudio.com/docs/sourcecontrol/github\" target=\"_blank\" rel=\"noopener noreferrer\">Collaborate on GitHub</a></p><hr><h2 id=\"method-2-using-github-codespaces-cloud-based-ide\" style=\"position:relative;\">Method 2: Using GitHub Codespaces (Cloud-Based IDE)<a href=\"#method-2-using-github-codespaces-cloud-based-ide\" aria-label=\"method 2 using github codespaces cloud based ide permalink\" class=\"anchor after\"><svg aria-hidden=\"true\" focusable=\"false\" height=\"16\" version=\"1.1\" viewBox=\"0 0 16 16\" width=\"16\"><path fill-rule=\"evenodd\" d=\"M4 9h1v1H4c-1.5 0-3-1.69-3-3.5S2.55 3 4 3h4c1.45 0 3 1.69 3 3.5 0 1.41-.91 2.72-2 3.25V8.59c.58-.45 1-1.27 1-2.09C10 5.22 8.98 4 8 4H4c-.98 0-2 1.22-2 2.5S3 9 4 9zm9-3h-1v1h1c1 0 2 1.22 2 2.5S13.98 12 13 12H9c-.98 0-2-1.22-2-2.5 0-.83.42-1.64 1-2.09V6.25c-1.09.53-2 1.84-2 3.25C6 11.31 7.55 13 9 13h4c1.45 0 3-1.69 3-3.5S14.5 6 13 6z\"></path></svg></a></h2><p><strong>GitHub Codespaces</strong> is a cloud-based development environment that allows you to code directly in your browser without installing anything locally.</p><h3 id=\"benefits-of-github-codespaces\" style=\"position:relative;\"><strong>Benefits of GitHub Codespaces:</strong><a href=\"#benefits-of-github-codespaces\" aria-label=\"benefits of github codespaces permalink\" class=\"anchor after\"><svg aria-hidden=\"true\" focusable=\"false\" height=\"16\" version=\"1.1\" viewBox=\"0 0 16 16\" width=\"16\"><path fill-rule=\"evenodd\" d=\"M4 9h1v1H4c-1.5 0-3-1.69-3-3.5S2.55 3 4 3h4c1.45 0 3 1.69 3 3.5 0 1.41-.91 2.72-2 3.25V8.59c.58-.45 1-1.27 1-2.09C10 5.22 8.98 4 8 4H4c-.98 0-2 1.22-2 2.5S3 9 4 9zm9-3h-1v1h1c1 0 2 1.22 2 2.5S13.98 12 13 12H9c-.98 0-2-1.22-2-2.5 0-.83.42-1.64 1-2.09V6.25c-1.09.53-2 1.84-2 3.25C6 11.31 7.55 13 9 13h4c1.45 0 3-1.69 3-3.5S14.5 6 13 6z\"></path></svg></a></h3><ul>\n<li>No installation required</li>\n<li>Access your code from any device</li>\n<li>Standardized development environment</li>\n</ul><h3 id=\"how-to-use-github-codespaces\" style=\"position:relative;\"><strong>How to Use GitHub Codespaces:</strong><a href=\"#how-to-use-github-codespaces\" aria-label=\"how to use github codespaces permalink\" class=\"anchor after\"><svg aria-hidden=\"true\" focusable=\"false\" height=\"16\" version=\"1.1\" viewBox=\"0 0 16 16\" width=\"16\"><path fill-rule=\"evenodd\" d=\"M4 9h1v1H4c-1.5 0-3-1.69-3-3.5S2.55 3 4 3h4c1.45 0 3 1.69 3 3.5 0 1.41-.91 2.72-2 3.25V8.59c.58-.45 1-1.27 1-2.09C10 5.22 8.98 4 8 4H4c-.98 0-2 1.22-2 2.5S3 9 4 9zm9-3h-1v1h1c1 0 2 1.22 2 2.5S13.98 12 13 12H9c-.98 0-2-1.22-2-2.5 0-.83.42-1.64 1-2.09V6.25c-1.09.53-2 1.84-2 3.25C6 11.31 7.55 13 9 13h4c1.45 0 3-1.69 3-3.5S14.5 6 13 6z\"></path></svg></a></h3><ol>\n<li>Go to your GitHub repository.</li>\n<li>Click the <strong>\"Code\"</strong> button and select the <strong>\"Codespaces\"</strong> tab.</li>\n<li>Click <strong>\"New codespace\"</strong> to launch a fully configured VS Code environment in your browser.</li>\n</ol><p><strong>Watch this short video on how to use GitHub Codespaces:</strong></p><iframe width=\"560\" height=\"315\" src=\"https://www.youtube.com/embed/SVaqUjBUdMA?si=nrMV2p9_zsw7Ry-d\" title=\"YouTube video player\" frameborder=\"0\" allow=\"accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share\" referrerpolicy=\"strict-origin-when-cross-origin\" allowfullscreen></iframe><p><strong>For more details, visit the official documentation:</strong>\n<a href=\"https://docs.github.com/en/codespaces\" target=\"_blank\" rel=\"noopener noreferrer\">GitHub Codespaces</a></p><hr><h3 id=\"which-method-should-you-use\" style=\"position:relative;\"><strong>Which Method Should You Use?</strong><a href=\"#which-method-should-you-use\" aria-label=\"which method should you use permalink\" class=\"anchor after\"><svg aria-hidden=\"true\" focusable=\"false\" height=\"16\" version=\"1.1\" viewBox=\"0 0 16 16\" width=\"16\"><path fill-rule=\"evenodd\" d=\"M4 9h1v1H4c-1.5 0-3-1.69-3-3.5S2.55 3 4 3h4c1.45 0 3 1.69 3 3.5 0 1.41-.91 2.72-2 3.25V8.59c.58-.45 1-1.27 1-2.09C10 5.22 8.98 4 8 4H4c-.98 0-2 1.22-2 2.5S3 9 4 9zm9-3h-1v1h1c1 0 2 1.22 2 2.5S13.98 12 13 12H9c-.98 0-2-1.22-2-2.5 0-.83.42-1.64 1-2.09V6.25c-1.09.53-2 1.84-2 3.25C6 11.31 7.55 13 9 13h4c1.45 0 3-1.69 3-3.5S14.5 6 13 6z\"></path></svg></a></h3><ul>\n<li><strong>Use the local method</strong> if you prefer working locally and need <strong>full control over your development environment</strong>.</li>\n<li><strong>Use GitHub Codespaces</strong> if you want to <strong>code from anywhere</strong> without worrying about installations.</li>\n</ul><h2 id=\"basic-git-command-line-reference\" style=\"position:relative;\">Basic Git Command Line Reference<a href=\"#basic-git-command-line-reference\" aria-label=\"basic git command line reference permalink\" class=\"anchor after\"><svg aria-hidden=\"true\" focusable=\"false\" height=\"16\" version=\"1.1\" viewBox=\"0 0 16 16\" width=\"16\"><path fill-rule=\"evenodd\" d=\"M4 9h1v1H4c-1.5 0-3-1.69-3-3.5S2.55 3 4 3h4c1.45 0 3 1.69 3 3.5 0 1.41-.91 2.72-2 3.25V8.59c.58-.45 1-1.27 1-2.09C10 5.22 8.98 4 8 4H4c-.98 0-2 1.22-2 2.5S3 9 4 9zm9-3h-1v1h1c1 0 2 1.22 2 2.5S13.98 12 13 12H9c-.98 0-2-1.22-2-2.5 0-.83.42-1.64 1-2.09V6.25c-1.09.53-2 1.84-2 3.25C6 11.31 7.55 13 9 13h4c1.45 0 3-1.69 3-3.5S14.5 6 13 6z\"></path></svg></a></h2><p>Here are some essential Git commands to get started:</p><table>\n<thead>\n<tr>\n<th>Command</th>\n<th>Description</th>\n</tr>\n</thead>\n<tbody>\n<tr>\n<td><code class=\"language-text\">git --version</code></td>\n<td>Check if Git is installed</td>\n</tr>\n<tr>\n<td><code class=\"language-text\">git config --global user.name \"Your Name\"</code></td>\n<td>Set your Git username</td>\n</tr>\n<tr>\n<td><code class=\"language-text\">git config --global user.email \"your-email@example.com\"</code></td>\n<td>Set your Git email (must match GitHub)</td>\n</tr>\n<tr>\n<td><code class=\"language-text\">git config --list</code></td>\n<td>View your Git configuration</td>\n</tr>\n<tr>\n<td><code class=\"language-text\">git init</code></td>\n<td>Initialize a new Git repository in a folder</td>\n</tr>\n<tr>\n<td><code class=\"language-text\">git clone &lt;repo-url></code></td>\n<td>Clone a repository from GitHub</td>\n</tr>\n<tr>\n<td><code class=\"language-text\">git status</code></td>\n<td>Check the status of your working directory</td>\n</tr>\n<tr>\n<td><code class=\"language-text\">git add &lt;file></code></td>\n<td>Stage a file for commit</td>\n</tr>\n<tr>\n<td><code class=\"language-text\">git add .</code></td>\n<td>Stage all modified files for commit</td>\n</tr>\n<tr>\n<td><code class=\"language-text\">git commit -m \"Your commit message\"</code></td>\n<td>Commit changes with a message</td>\n</tr>\n<tr>\n<td><code class=\"language-text\">git log --oneline --graph --all</code></td>\n<td>View commit history in a readable format</td>\n</tr>\n<tr>\n<td><code class=\"language-text\">git branch</code></td>\n<td>List all branches in the repository</td>\n</tr>\n<tr>\n<td><code class=\"language-text\">git checkout -b &lt;branch-name></code></td>\n<td>Create and switch to a new branch</td>\n</tr>\n<tr>\n<td><code class=\"language-text\">git merge &lt;branch-name></code></td>\n<td>Merge a branch into the current branch</td>\n</tr>\n<tr>\n<td><code class=\"language-text\">git push origin &lt;branch-name></code></td>\n<td>Push changes to GitHub</td>\n</tr>\n<tr>\n<td><code class=\"language-text\">git pull origin &lt;branch-name></code></td>\n<td>Pull the latest changes from GitHub</td>\n</tr>\n<tr>\n<td><code class=\"language-text\">git remote -v</code></td>\n<td>Show linked remote repositories</td>\n</tr>\n<tr>\n<td><code class=\"language-text\">git reset --hard &lt;commit-id></code></td>\n<td>Reset to a specific commit (dangerous, use with caution)</td>\n</tr>\n</tbody>\n</table><p><strong>For a complete list of Git commands, check the official Git documentation:</strong>\n<a href=\"https://git-scm.com/docs\" target=\"_blank\" rel=\"noopener noreferrer\">Git Command Reference</a></p></div>","frontmatter":{"path":"/part-7/2-vscode","title":"Integrating GitHub with VS Code"},"fileAbsolutePath":"/home/runner/work/programming-24/programming-24/data/part-7/2-vscode.md"},"allPages":{"edges":[{"node":{"id":"a6686aff-fa9a-58c9-8b0e-9fe4317c0c3b"